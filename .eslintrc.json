{
    "env": {
        "node": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-useless-concat": "off",
        "no-console": "off",
        "linebreak-style": "off"
    }
}
